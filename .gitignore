# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# dependencies
/node_modules
/.pnp
.pnp.js

# testing
/coverage

# production
/build

# misc
.DS_Store
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

npm-debug.log*
yarn-debug.log*
yarn-error.log*

# backend
/spotify-together-api/.bundle

# Ignore the default SQLite database.
/spotify-together-api/db/*.sqlite3
/spotify-together-api/db/*.sqlite3-*

/spotify-together-api/config/database.yml
# Ignore all logfiles and tempfiles.
/spotify-together-api/spotify-together-apilog/*
/spotify-together-api/tmp/*
!/spotify-together-api/log/.keep
!/spotify-together-api/tmp/.keep

# Ignore pidfiles, but keep the directory.
/spotify-together-api/tmp/pids/*
!/spotify-together-api/tmp/pids/
!/spotify-together-api/tmp/pids/.keep

# Ignore uploaded files in development.
/spotify-together-api/storage/*
!/spotify-together-api/storage/.keep
/spotify-together-api/.byebug_history

# Ignore master key for decrypting credentials and more.
/spotify-together-api/config/master.key

